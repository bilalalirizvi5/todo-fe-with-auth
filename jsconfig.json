{
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "@config": ["config"],
      "@screens": ["screens"],
      "@components": ["components"],
      "@styledComponents/*": ["components/styledComponents/*"],
      "@images/*": ["assets/images/*"],
      "@redux/*": ["redux/*"],
      "@slices/*": ["redux/slices/*"],
      "@services/*": ["redux/services/*"]
    }
  },
  "exclude": ["node_modules"]
}
